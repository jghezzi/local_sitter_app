.row
	.col-md-4
		= f.label :status
	.col-md-4	
		= f.text_area :status
.row
	.col-md-4
		= f.label :date
	.col-md-4
		= f.date_field :date
.row
	.col-md-4
		= f.label :start_time
	.col-md-4
		= f.time_field :start_time
.row
	.col-md-4
		= f.label :end_time
	.col-md-4
		= f.time_field :end_time
.row
	.col-md-4
		= f.label :family_id
	.col-md-4
		= f.select :family_id, Family.all.collect{|a| [a.name, a.id]}
.row
	.col-md-4
		= f.label :sitter_id
	.col-md-4
		= f.select :sitter_id, Sitter.all.collect{|a| [a.name, a.id] }
.row
	.col-md-4
		= f.submit "Submit"






